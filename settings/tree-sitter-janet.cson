'.source.janet':
  'editor':
    'autoIndentOnPaste': false
    'commentStart': '# '
    'tabLength': 2
    'softTabs': true
    'preferredLineLength': 80
  'autocomplete':
    'extraWordCharacters': '-'
  'bracket-matcher':
     autocompleteCharacters: [
       '()'
       '[]'
       '{}'
       '""'
       '``'
     ]
